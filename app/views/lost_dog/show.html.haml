= form_for :lostdog do |f|
  .row-fluid
  .offset2.span7
    %div.well
      %p
        %b #{t('dog.name')}:
        = @lostdog.name
      %p
        %b #{t('dog.age')}:
        = @lostdog.age
      %p
        %b #{t('dog.breed')}:
        = @lostdog.breed
      %p
        %b #{t('dog.color')}:
        = @lostdog.color
      %p
        %b #{t('dog.description')}:
        = @lostdog.description

      = link_to "#{t('atras')}", lost_dog_index_path
